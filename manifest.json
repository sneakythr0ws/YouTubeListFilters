{
  "manifest_version": 2,
  "name": "Chrome YouTube Filters",
  "description": "Chrome YouTube Filters",
  "version": "1",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}